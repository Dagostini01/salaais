{
  "expo": {
    "name": "Sala AIS",
    "slug": "salaais",
    "scheme": "salaais",
    "version": "1.3.1",
    "orientation": "portrait",
    "icon": "./src/assets/logo-amarelo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/logo-amarelo-removebg.png",
      "resizeMode": "contain",
      "backgroundColor": "#fbd434"
    },
    "ios": {
      "buildNumber": "1.3.1",
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.sala.ais",
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) precisa da sua permissão para usar a câmera para autenticar com o Apple."
      }
    },
    "android": {
      "versionCode": 14,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo-amarelo-removebg.png",
        "backgroundColor": "#fbcb04"
      },
      "package": "com.salaais"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a2ad3ada-0b73-4c68-86f4-c5af0cf62806"
      }
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      "./plugins",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.178145981087-g3719f08ontptnscrjco6brli473dd48"
        }
      ]
    ]
  }
}

{
  "expo": {
    "name": "Sala AIS",
    "slug": "salaais",
    "sheme:": "salaais",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo-amarelo.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/logo-amarelo-removebg.png",
      "resizeMode": "contain",
      "backgroundColor": "#fbd434"
    },
    "ios": {
      "buildNumber": "1.0.0",
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.sala.ais",
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) precisa da sua permissão para usar a câmera para autenticar com o Apple."
      }
    },
    "android": {
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo-amarelo-removebg.png",
        "backgroundColor": "#fbcb04"
      },
      "package": "com.salaais"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a2ad3ada-0b73-4c68-86f4-c5af0cf62806"
      }
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-secure-store",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.salaais"
        }
      ]
    ]
  }
}
